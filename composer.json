{
    "name": "sierratecnologia/builder",
    "description": "Prepare your Laravel apps incredibly fast, with various commands, services, facades and boilerplates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Sierra",
            "email": "contato@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.5",
        "illuminate/support": "^5.7",
        "sierratecnologia/crudmaker": "dev-master",
        "sierratecnologia/formmaker": "dev-master",
        "sierratecnologia/crypto": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "SierraTecnologia\\Builder\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SierraTecnologia\\Builder\\SierraTecnologiaBuilderProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
